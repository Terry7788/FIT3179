{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Line Chart of Enrollment by Age Group and Sex with Distinct Points, Tooltip, and Centered Title",
    "title": {
      "text": "Enrollment in Study by Age Group and Sex (2023)",
      "anchor": "middle",
      "fontSize": 16
    },
    "width": 1000,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/Terry7788/FIT3179/refs/heads/main/Data/updated_enrollment_by_age_and_sex.csv"
    },
    "transform": [
      {
        "fold": ["Male (%)", "Female (%)"],
        "as": ["Sex", "Percentage"]
      }
    ],
    "mark": {
      "type": "line",
      "point": {
        "filled": true,
        "size": 100
      }
    },
    "encoding": {
      "x": {
        "field": "Age Group",
        "type": "ordinal",
        "axis": {"title": "Age Group", "labelAngle": 0}
      },
      "y": {
        "field": "Percentage",
        "type": "quantitative",
        "axis": {"title": "Enrollment Percentage"}
      },
      "color": {
        "field": "Sex",
        "type": "nominal",
        "legend": {"title": "Sex"}
      },
      "tooltip": [
        {"field": "Age Group", "type": "ordinal", "title": "Age Group"},
        {"field": "Sex", "type": "nominal", "title": "Sex"},
        {"field": "Percentage", "type": "quantitative", "title": "Enrollment Percentage"}
      ]
    }
  }
  